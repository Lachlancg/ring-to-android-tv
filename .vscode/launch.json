    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\app.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "List Locations & Cameras",
            "program": "${workspaceFolder}/app.js",
            "args": ["--list"],
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Get Snapshot - Default",
            "program": "${workspaceFolder}/app.js",
            "args": ["--test"],
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Get Snapshot - 0,1",
            "program": "${workspaceFolder}/app.js",
            "args": ["--test", "0,1"],
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
    ]
}